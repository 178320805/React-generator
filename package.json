    "webpack-hot-middleware": "^2.21.0",
    "webpack-merge": "^4.1.1",
    "why-did-you-update": "^0.1.0"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
