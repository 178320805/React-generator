{
  "name": "wx-pwa",
  "version": "1.0.0",
  "description": "wx pwa generator",
  "scripts": {
    "start": "better-npm-run start:dev",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1"
  },
  "betterScripts": {
    "start:dev": {
      "command": "webpack --config build/webpack.dll.conf.js && node build/server/dev.server.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "license": "MIT",
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.1.2",
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "better-npm-run": "^0.1.0",
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^1.2.0",
    "commitizen": "^2.9.6",
    "conventional-changelog-cli": "^1.3.5",
    "css-loader": "^0.28.7",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.11.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.5.1",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^3.0.2",
    "happypack": "^4.0.0",
    "html-webpack-plugin": "^2.30.1",
    "i18n-webpack-plugin": "^1.0.0",
    "json-loader": "^0.5.7",
    "koa": "^2.4.1",
    "koa-static": "^4.0.2",
    "koa-webpack": "^1.0.0",
    "node-sass": "^4.7.2",
    "nyan-progress-webpack-plugin": "^1.2.0",
    "postcss-loader": "^2.0.9",
    "react-hot-loader": "^3.1.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-order": "^0.7.0",
    "stylelint-scss": "^2.1.0",
    "validate-commit-msg": "^2.14.0",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-dev-server": "^2.9.4",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-merge": "^4.1.1",
    "why-did-you-update": "^0.1.0"
  },
  "dependencies": {
    "fastclick": "^1.0.6",
    "immutability-helper": "^2.5.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "styled-components": "^2.2.4"
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
