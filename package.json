{
  "name": "wx-pwa",
  "version": "1.0.0",
  "description": "wx pwa generator",
  "scripts": {
    "start": "better-npm-run start:dev",
    "dll": "better-npm-run start:dll"
  },
  "betterScripts": {
    "start:dll": {
      "command": "webpack --config build/webpack.dll.conf.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start:dev": {
      "command": "webpack --config build/webpack.dll.conf.js && node build/server/dev.server.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "license": "MIT",
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.1.2",
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "better-npm-run": "^0.1.0",
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^1.2.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.5.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "happypack": "^4.0.0",
    "html-webpack-plugin": "^2.30.1",
    "json-loader": "^0.5.7",
    "node-sass": "^4.7.2",
    "nyan-progress-webpack-plugin": "^1.2.0",
    "postcss-loader": "^2.0.9",
    "react-hot-loader": "^3.1.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-order": "^0.7.0",
    "stylelint-scss": "^2.1.0",
    "webpack": "^3.8.1",
